   float4 mClipPosition: TEXCOORD1;
};

float4 ps_main(PS_INPUT Input) : COLOR
{ 
 
   float depth = Input.mClipPosition.z / Input.mClipPosition.w;
   
   float2 uv = Input.mClipPosition.xy / Input.mClipPosition.w;
   uv.y = -uv.y;
   uv = uv * 0.5 + 0.5;
   
   
   return float4(depth.xxx, 1);

}
